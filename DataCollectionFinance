l):\n","    \"\"\"Extract company name from PRNewswire URL.\"\"\"\n","    # Pattern for PRNewswire URLs like https://www.prnewswire.com/news/microsoft-corp./\n","    match = re.search(r'/news/([^/]+)', url)\n","    if match:\n","        return match.group(1).replace('-', '_')\n","    return \"unknown_company\"\n","\n","def is_english(text):\n","    \"\"\"Check if the text is in English.\"\"\"\n","    try:\n","        return langdetect.detect(text) == 'en'\n","    except:\n","        # If detection fails, assume it's English\n","        return True\n","\n","def scrape_prnewswire(url, max_articles=15):\n","    \"\"\"\n","    Scrape press releases from PRNewswire for a specific company.\n","\n","    Args:\n","        url: The PRNewswire URL for the company\n","        max_articles: Maximum number of articles to scrape (default: 15)\n","    \"\"\"\n","    company_name = extract_company_name(url)\n","    print(f\"Scraping PRNewswire releases for {company_name}...\")\n","\n","    # Create directory structure\n","    company_dir = create_directory_structure(company_name)\n","\n","    # Send HTTP request to the URL\n","    try:\n","        headers = {\n","            'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/91.0.4472.124 Safari/537.36'\n","        }\n","        response = requests.get(url, headers=headers)\n","        response.raise_for_status()\n","    except requests.exceptions.RequestException as e:\n","        print(f\"Error fetching the URL: {e}\")\n","        return\n","\n","    # Parse HTML content\n","    soup = BeautifulSoup(response.text, 'html.parser')\n","\n","    # Find article links - PRNewswire typically has news cards with links\n","    article_links = []\n","\n","    # Look for news cards\n","    news_cards = soup.find_all('div', class_='card')\n","\n","    if not news_cards:\n","        # Alternative: try to find link elements with news URLs\n","        all_links = soup.find_all('a')\n","        for link in all_links:\n","            href = link.get('href')\n","            if href and re.search(r'/news-releases/', href):\n","                full_url = urljoin('https://www.prnewswire.com', href)\n","                article_links.append(full_url)\n","    else:\n","        for card in news_cards:\n","            link_elem = card.find('a')\n","            if link_elem and link_elem.get('href'):\n","                href = link_elem.get('href')\n","                full_url = urljoin('https://www.prnewswire.com', href)\n","                article_links.append(full_url)\n","\n","    # Limit to the specified number of articles\n","    article_links = article_links[:max_articles]\n","\n","    print(f\"Found {len(article_links)} articles to scrape.\")\n","\n","    # Process each article\n","    saved_count = 0\n","    for i, article_url in enumerate(article_links):\n","        try:\n","            # Add delay between requests\n","            time.sleep(2)\n","\n","            # Fetch article content\n","            article_response = requests.get(article_url, headers=headers)\n","            article_response.raise_for_status()\n","\n","            # Parse article content\n","            article_soup = BeautifulSoup(article_response.text, 'html.parser')\n","\n","            # Extract title\n","            title_element = article_soup.find(['h1'])\n","            title = title_element.text.strip() if title_element else f\"Article_{i+1}\"\n","\n","            # Extract article content - PRNewswire typically has a release-text div\n","            content_element = article_soup.find('div', {'class': 'release-text'})\n","\n","            if not content_element:\n","                # Fallback to paragraphs\n","                content_element = article_soup.find('section', {'class': 'news-release'})\n","\n","            if content_element:\n","                paragraphs = content_element.find_all('p')\n","                content = \"\\n\\n\".join([p.text.strip() for p in paragraphs])\n","            else:\n","                # Last resort: get all paragraphs from the page\n","                paragraphs = article_soup.find_all('p')\n","                content = \"\\n\\n\".join([p.text.strip() for p in paragraphs])\n","\n","            # Check if content is in English\n","            if not is_english(content[:1000]):  # Check first 1000 chars to be efficient\n","                print(f\"Skipping non-English article: {title}\")\n","                continue\n","\n","            # Create a valid filename\n","            filename = \"\".join(c if c.isalnum() or c in [' ', '-', '_'] else '_' for c in title)\n","            filename = filename[:50] + \".txt\"  # Limit filename length\n","\n","            # Prepare full content\n","            full_content = f\"Title: {title}\\nURL: {article_url}\\n\\n{content}\"\n","\n","            # Check file size (approximately)\n","            estimated_size = len(full_content.encode('utf-8'))\n","            if estimated_size < 1024:  # 1KB\n","                print(f\"Skipping article smaller than 1KB: {title}\")\n","                continue\n","\n","            # Save content to file\n","            file_path = os.path.join(company_dir, filename)\n","            with open(file_path, 'w', encoding='utf-8') as f:\n","                f.write(full_content)\n","\n","            print(f\"Saved: {filename}\")\n","            saved_count += 1\n","\n","        except Exception as e:\n","            print(f\"Error processing article {article_url}: {e}\")\n","\n","    print(f\"Scraping completed. {saved_count} files saved in {company_dir}\")\n","    return company_dir\n","\n","# Function to download all files as a zip\n","def download_files(company_dir):\n","    \"\"\"Create a zip file of the scraped press releases and download it.\"\"\"\n","    try:\n","        # Create zip file of the directory\n","        !zip -r {company_dir}.zip {company_dir}\n","\n","        # Download the zip file\n","        files.download(f\"{company_dir}.zip\")\n","        print(f\"Downloaded {company_dir}.zip\")\n","    except Exception as e:\n","        print(f\"Error creating or downloading zip file: {e}\")\n","        print(\"You can manually download files from the file browser on the left sidebar\")\n","\n","# Interactive input cell\n","url = input(\"Enter PRNewswire company URL (e.g., https://www.prnewswire.com/news/microsoft-corp./): \")\n","max_articles = int(input(\"Enter maximum number of articles to scrape (default is 15): \") or \"15\")\n","\n","# Run the scraper\n","company_dir = scrape_prnewswire(url, max_articles)\n","\n","# Option to download files\n","if input(\"Download files as zip? (y/n): \").lower() == 'y':\n","    download_files(company_dir)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"5uLScsnsH-Kr","executionInfo":{"status":"ok","timestamp":1741477645647,"user_tz":300,"elapsed":49500,"user":{"displayName":"Caden Webb","userId":"00439241620104196769"}},"outputId":"40ccaf5d-1631-46c9-f030-8045702f2aee"},"execution_count":null,"outputs":[{"name":"stdout","output_type":"stream","text":["Enter PRNewswire company URL (e.g., https://www.prnewswire.com/news/microsoft-corp./): https://www.prnewswire.com/news/rtx/\n","Enter maximum number of articles to scrape (default is 15): 15\n","Scraping PRNewswire releases for rtx...\n","Found 15 articles to scrape.\n","Saved: RTX_s Pratt _ Whitney and Collins Aerospace to lea.txt\n","Saved: RTX_s Pratt _ Whitney completes series of rotating.txt\n","Saved: RTX_s Pratt _ Whitney F135 engine surpasses 1 mill.txt\n","Saved: RTX_s Collins Aerospace EPACS power and thermal ma.txt\n","Saved: RTX_s Pratt _ Whitney GTF Advantage_ engine achiev.txt\n","Saved: RTX_s Collins Aerospace selected by Boeing to prov.txt\n","Saved: RTX_s Raytheon demonstrates first-ever AI_ML-power.txt\n","Saved: NSPA awards RTX additional contract for optical da.txt\n","Saved: EASA certifies Airbus A321XLR aircraft powered by .txt\n","Saved: RTX_s Pratt _ Whitney passes first fully digital d.txt\n","Saved: RTX_s Pratt _ Whitney awarded _1_5 billion F119 en.txt\n","Saved: RTX_s Raytheon completes 10 subsystem demonstratio.txt\n","Saved: RTX_s Pratt _ Whitney Adds Sanad Group to the GTF .txt\n","Saved: RTX_s Raytheon Conducts Successful Test with LTAMD.txt\n","Saved: RTX_s Collins Aerospace launches Powered by Collin.txt\n","Scraping completed. 15 files saved in PR/rtx\n","Download files as zip? (y/n): n\n"]}]},{"cell_type":"markdown","source":["# 10 K text"],"metadata":{"id":"sa-2z0wMwaqI"}},{"cell_type":"markdown","source":["## All companies"],"metadata":{"id":"w9d5ZUD5Lesf"}},{"cell_type":"markdown","source":["### Claude"],"metadata":{"id":"1upg-G-NMn6D"}},{"cell_type":"code","source":["import os\n","import requests\n","import time\n","import re\n","import json\n","import unicodedata\n","from bs4 import BeautifulSoup\n","from datetime import datetime\n","\n","# List of companies to process\n","companies = ['AAPL', 'MSFT', 'GOOGL', 'AMZN', 'META', 'NVDA', 'TSLA', 'JPM', 'RTX', 'WMT', 'V']\n","\n","# Dictionary mapping tickers to CIK numbers\n","CIK_MAPPING = {\n","    'AAPL': '0000320193',\n","    'MSFT': '0000789019',\n","    'GOOGL': '0001652044',\n","    'AMZN': '0001018724',\n","    'META': '0001326801',\n","    'NVDA': '0001045810',\n","    'TSLA': '0001318605',\n","    'JPM': '0000019617',\n","    'RTX': '0001047122',\n","    'WMT': '0000104169',\n","    'V': '0001403161'\n","}\n","\n","def get_10k_links(ticker, count=8):\n","    \"\"\"Get the 10-K filing links for a specific company ticker\"\"\"\n","    cik = CIK_MAPPING.get(ticker)\n","    if not cik:\n","        print(f\"CIK number not found for ticker: {ticker}\")\n","        return []\n","\n","    url = f\"https://www.sec.gov/cgi-bin/browse-edgar?action=getcompany&CIK={cik}&type=10-K&count={count}\"\n","    headers = {\"User-Agent\": \"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36\"}\n","\n","    try:\n","        response = requests.get(url, headers=headers)\n","        response.raise_for_status()\n","        soup = BeautifulSoup(response.text, 'html.parser')\n","\n","        # Extract document links\n","        filing_links = []\n","        for table in soup.find_all('table', class_='tableFile2'):\n","            for row in table.find_all('tr'):\n","                cells = row.find_all('td')\n","                if len(cells) > 3 and cells[0].text.strip() == '10-K':\n","                    doc_link = cells[1].a['href']\n","                    filing_links.append(f\"https://www.sec.gov{doc_link}\")\n","\n","        return filing_links[:count]  # Return only the specified most recent filings\n","    except Exception as e:\n","        print(f\"Error retrieving 10-K links for {ticker}: {e}\")\n","        return []\n","\n","def extract_10k_text(doc_url):\n","    \"\"\"Extract text from a 10-K filing URL\"\"\"\n","    headers = {\"User-Agent\": \"Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36\"}\n","\n","    try:\n","        response = requests.get(doc_url, headers=headers)\n","        response.raise_for_status()\n","        soup = BeautifulSoup(response.text, 'html.parser')\n","\n","        # Find the actual document link\n","        table = soup.find('table', class_='tableFile')\n","        if table:\n","            for row in table.find_all('tr'):\n","                cells = row.find_all('td')\n","                if len(cells) >= 3 and \"Complete submission\" in cells[1].text:\n","                    txt_url = f\"https://www.sec.gov{cells[2].a['href']}\"\n","\n","                    # Get the text document\n","                    time.sleep(1)  # Be nice to SEC servers\n","                    txt_response = requests.get(txt_url, headers=headers)\n","                    txt_response.raise_for_status()\n","                    return txt_response.text\n","    except Exception as e:\n","        print(f\"Error extracting text from {doc_url}: {e}\")\n","\n","    return None\n","\n","def clean_text(text):\n","    \"\"\"Clean and normalize the 10-K filing text\"\"\"\n","    # First pass with BeautifulSoup to handle HTML/XML\n","    soup = BeautifulSoup(text, \"html.parser\")\n","    text = soup.get_text(separator=\" \")\n","\n","    # Remove common XBRL namespace links often found at start of documents\n","    text = re.sub(r'http://fasb\\.org/us-gaap/[0-9]+#\\w+', '', text)\n","    text = re.sub(r'http://xbrl\\.sec\\.gov/[a-z]+/[0-9]+#\\w+', '', text)\n","    text = re.sub(r'http://www\\.xbrl\\.org/[0-9]+/\\w+', '', text)\n","\n","    # Remove style attributes and CSS\n","    text = re.sub(r'style=\"[^\"]*\"', '', text)\n","    text = re.sub(r'class=\"[^\"]*\"', '', text)\n","    text = re.sub(r'font-family:[^;\"]*;?', '', text)\n","    text = re.sub(r'color:[^;\"]*;?', '', text)\n","    text = re.sub(r'background-color:[^;\"]*;?', '', text)\n","\n","    # Remove HTML tags and attributes more aggressively\n","    text = re.sub(r'<[^>]*>', '', text)\n","\n","    # Remove URLs, email addresses and links\n","    text = re.sub(r'http[s]?://(?:[a-zA-Z]|[0-9]|[$-_@.&+]|[!*\\\\(\\\\),]|(?:%[0-9a-fA-F][0-9a-fA-F]))+', '', text)\n","    text = re.sub(r'[\\w.+-]+@[\\w-]+\\.(?:[\\w-]\\.?)+[\\w-]', '', text)\n","    text = re.sub(r'\\[link\\].*?\\[/link\\]', '', text)\n","\n","    # Remove HTML entities\n","    text = re.sub(r'&[a-zA-Z0-9#]+;', ' ', text)\n","\n","    # Remove binary and hex data patterns\n","    text = re.sub(r'0x[0-9a-fA-F]+', '', text)\n","    text = re.sub(r'\\\\x[0-9a-fA-F]{2}', '', text)\n","\n","    # Remove EDGAR-specific patterns\n","    text = re.sub(r'\\[\\S+ GRAPHIC\\]', '', text)\n","    text = re.sub(r'\\[\\S+ TABLE\\]', '', text)\n","    text = re.sub(r'\\[\\S+ IMAGE\\]', '', text)\n","    text = re.sub(r'\\[\\S+ CHART\\]', '', text)\n","\n","    # Remove long alphanumeric sequences that are likely IDs or encoded data\n","    text = re.sub(r'[A-Z0-9]{10,}', '', text)\n","\n","    # Remove base64 and other encoded data patterns\n","    text = re.sub(r'[a-zA-Z0-9+/]{30,}={0,2}', '', text)\n","    text = re.sub(r'M[A-Z0-9\\'=_+:;$%&@!*()#/<>\\[\\]]{5,}', '', text)\n","\n","    # Remove potential embedded image data patterns\n","    text = re.sub(r'data:image/[a-zA-Z0-9+/=;:,]+', '', text)\n","    text = re.sub(r'[A-Za-z0-9+/=]{50,}', '', text)  # Likely base64 encoded image\n","\n","    # Remove sections likely to be binary/image data (common patterns in SEC filings)\n","    text = re.sub(r'begin 644[^a-zA-Z0-9]*.*?end', '', text, flags=re.DOTALL)\n","    text = re.sub(r'MIME-Version:.*?Content-Type:', '', text, flags=re.DOTALL)\n","\n","    # Remove lines that contain only URLs, especially at document start\n","    lines = text.split('\\n')\n","    filtered_lines = []\n","    for line in lines:\n","        # Skip lines that are entirely URLs or mostly URLs\n","        if not re.match(r'^[\\s]*http[s]?://', line) and not re.match(r'^[\\s]*[a-zA-Z0-9.]+\\.[a-zA-Z]{2,}', line):\n","            if not re.search(r'http[s]?://', line) or len(re.findall(r'http[s]?://', line)) < 3:\n","                filtered_lines.append(line)\n","\n","    text = '\\n'.join(filtered_lines)\n","\n","    # Remove non-printable and control characters\n","    text = ''.join(c for c in text if c.isprintable() and not unicodedata.category(c).startswith('C'))\n","\n","    # Normalize Unicode characters\n","    text = unicodedata.normalize('NFKD', text)\n","\n","    # Clean up whitespace\n","    text = re.sub(r'\\s+', ' ', text)\n","    text = re.sub(r'\\n\\s*\\n', '\\n\\n', text)\n","\n","    # Remove lines with very low information content\n","    lines = text.split('\\n')\n","    filtered_lines = []\n","    for line in lines:\n","        line = line.strip()\n","        # Skip empty lines or lines with just punctuation/symbols\n","        if line and not re.match(r'^[\\s\\d\\W]+$', line):\n","            # Skip lines that are just XBRL taxonomy references\n","            if not re.match(r'^(us-gaap|dei|xbrl|fasb|srt|country)', line, re.IGNORECASE):\n","                filtered_lines.append(line)\n","\n","    # Remove duplicate consecutive lines\n","    unique_lines = []\n","    prev_line = None\n","    for line in filtered_lines:\n","        if line != prev_line:\n","            unique_lines.append(line)\n","            prev_line = line\n","\n","    text = '\\n'.join(unique_lines)\n","\n","    # Final cleanup: remove any remaining style/css fragments\n","    text = re.sub(r'(background|text-align|vertical-align|font|padding|margin|border|width|height):[^;]+;', '', text)\n","\n","    return text.strip()\n","\n","def extract_metadata(filename, ticker):\n","    \"\"\"Extract metadata from a filename\"\"\"\n","    match = re.search(rf'{ticker.lower()}_10k_(\\d+)', filename.lower())\n","    filing_num = int(match.group(1)) if match else 0\n","    current_year = datetime.now().year\n","    estimated_year = current_year - (filing_num - 1)\n","\n","    # Map tickers to company names\n","    company_names = {\n","        'AAPL': 'Apple Inc.',\n","        'MSFT': 'Microsoft Corporation',\n","        'GOOGL': 'Alphabet Inc.',\n","        'AMZN': 'Amazon.com Inc.',\n","        'META': 'Meta Platforms Inc.',\n","        'NVDA': 'NVIDIA Corporation',\n","        'TSLA': 'Tesla Inc.',\n","        'JPM': 'JPMorgan Chase & Co.',\n","        'RTX': 'Raytheon Technologies Corporation',\n","        'WMT': 'Walmart Inc.',\n","        'V': 'Visa Inc.'\n","    }\n","\n","    return {\n","        \"filing_number\": filing_num,\n","        \"year\": estimated_year,\n","        \"company\": company_names.get(ticker, f\"{ticker} Inc.\"),\n","        \"ticker\": ticker,\n","        \"document_type\": \"10-K\"\n","    }\n","\n","def chunk_text(text, max_chunk_size=1000, overlap=100):\n","    \"\"\"Split text into chunks for RAG processing\"\"\"\n","    words = text.split()  # Tokenize by words\n","    chunks = []\n","    start = 0\n","\n","    while start < len(words):\n","        end = start + max_chunk_size\n","        chunk = \" \".join(words[start:min(end, len(words))])\n","        chunks.append(chunk)\n","        start = end - overlap  # Move back by overlap to maintain context\n","\n","    return chunks\n","\n","def create_rag_json(input_dir, ticker):\n","    \"\"\"Create RAG JSON from cleaned 10-K files for a specific ticker\"\"\"\n","    results = []\n","\n","    for filename in os.listdir(input_dir):\n","        if filename.endswith(\"_clean.txt\") and filename.startswith(f\"{ticker.lower()}_10k_\"):\n","            input_path = os.path.join(input_dir, filename)\n","            print(f\"Processing {filename} for RAG...\")\n","            metadata = extract_metadata(filename, ticker)\n","\n","            with open(input_path, 'r', encoding='utf-8') as f:\n","                text = f.read()\n","\n","            chunks = chunk_text(text)\n","\n","            for i, chunk in enumerate(chunks):\n","                doc = {\n","                    \"id\": f\"{ticker.lower()}-10k-{metadata['year']}-chunk-{i+1}\",\n","                    \"text\": chunk,\n","                    \"metadata\": {\n","                        **metadata,\n","                        \"chunk_number\": i+1,\n","                        \"total_chunks\": len(chunks),\n","                        \"processed_date\": datetime.now().strftime(\"%Y-%m-%d\"),\n","                        \"content_length\": len(chunk)\n","                    }\n","                }\n","                results.append(doc)\n","\n","    output_path = os.path.join(input_dir, f\"{ticker.lower()}_10k_rag.json\")\n","    with open(output_path, 'w', encoding='utf-8') as f:\n","        json.dump(results, f, indent=2)\n","\n","    print(f\"Created RAG JSON file with {len(results)} chunks at {output_path}\")\n","\n","def process_company(ticker, count=8):\n","    \"\"\"Process 10-K filings for a specific company ticker\"\"\"\n","    print(f\"\\n{'='*50}\")\n","    print(f\"Processing 10-K filings for {ticker}\")\n","    print(f\"{'='*50}\")\n","\n","    # Create output directory\n","    output_dir = f\"{ticker.lower()}_10k_filings\"\n","    os.makedirs(output_dir, exist_ok=True)\n","\n","    # Get 10-K filing links\n","    filing_links = get_10k_links(ticker, count)\n","\n","    if not filing_links:\n","        print(f\"No 10-K filings found for {ticker}\")\n","        return\n","\n","    print(f\"Found {len(filing_links)} 10-K filings for {ticker}\")\n","\n","    # Download and save each filing\n","    for i, link in enumerate(filing_links):\n","        print(f\"Processing filing {i+1}/{len(filing_links)}: {link}\")\n","        time.sleep(1)  # Respect SEC rate limits\n","\n","        filing_text = extract_10k_text(link)\n","        if filing_text:\n","            raw_file_path = os.path.join(output_dir, f\"{ticker.lower()}_10k_{i+1}.txt\")\n","            with open(raw_file_path, \"w\", encoding=\"utf-8\") as f:\n","                f.write(filing_text)\n","            print(f\"Saved raw filing {i+1} to {raw_file_path}\")\n","\n","            # Clean the text\n","            cleaned_text = clean_text(filing_text)\n","            clean_file_path = os.path.join(output_dir, f\"{ticker.lower()}_10k_{i+1}_clean.txt\")\n","            with open(clean_file_path, \"w\", encoding=\"utf-8\") as f:\n","                f.write(cleaned_text)\n","            print(f\"Saved cleaned filing {i+1} to {clean_file_path}\")\n","        else:\n","            print(f\"Failed to extract text from {link}\")\n","\n","    # Create RAG JSON after all files are processed\n","    create_rag_json(output_dir, ticker)\n","\n","def main():\n","    \"\"\"Main function to process all companies\"\"\"\n","    for ticker in companies:\n","        process_company(ticker)\n","        time.sleep(2)  # Add delay between companies to respect SEC rate limits\n","\n","if __name__ == \"__main__\":\n","    main()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"BTxCO7yAMpRu","executionInfo":{"status":"ok","timestamp":1741479210531,"user_tz":300,"elapsed":859711,"user":{"displayName":"Caden Webb","userId":"00439241620104196769"}},"outputId":"8aa88a5b-e81a-40a2-9635-f324698cc1ad"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","==================================================\n","Processing 10-K filings for AAPL\n","==================================================\n","Found 8 10-K filings for AAPL\n","Processing filing 1/8: https://www.sec.gov/Archives/edgar/data/320193/000032019324000123/0000320193-24-000123-index.htm\n","Saved raw filing 1 to aapl_10k_filings/aapl_10k_1.txt\n","Saved cleaned filing 1 to aapl_10k_filings/aapl_10k_1_clean.txt\n","Processing filing 2/8: https://www.sec.gov/Archives/edgar/data/320193/000032019323000106/0000320193-23-000106-index.htm\n","Saved raw filing 2 to aapl_10k_filings/aapl_10k_2.txt\n","Saved cleaned filing 2 to aapl_10k_filings/aapl_10k_2_clean.txt\n","Processing filing 3/8: https://www.sec.gov/Archives/edgar/data/320193/000032019322000108/0000320193-22-000108-index.htm\n","Saved raw filing 3 to aapl_10k_filings/aapl_10k_3.txt\n","Saved cleaned filing 3 to aapl_10k_filings/aapl_10k_3_clean.txt\n","Processing filing 4/8: https://www.sec.gov/Archives/edgar/data/320193/000032019321000105/0000320193-21-000105-index.htm\n","Saved raw filing 4 to aapl_10k_filings/aapl_10k_4.txt\n","Saved cleaned filing 4 to aapl_10k_filings/aapl_10k_4_clean.txt\n","Processing filing 5/8: https://www.sec.gov/Archives/edgar/data/320193/000032019320000096/0000320193-20-000096-index.htm\n","Saved raw filing 5 to aapl_10k_filings/aapl_10k_5.txt\n","Saved cleaned filing 5 to aapl_10k_filings/aapl_10k_5_clean.txt\n","Processing filing 6/8: https://www.sec.gov/Archives/edgar/data/320193/000032019319000119/0000320193-19-000119-index.htm\n","Saved raw filing 6 to aapl_10k_filings/aapl_10k_6.txt\n","Saved cleaned filing 6 to aapl_10k_filings/aapl_10k_6_clean.txt\n","Processing filing 7/8: https://www.sec.gov/Archives/edgar/data/320193/000032019