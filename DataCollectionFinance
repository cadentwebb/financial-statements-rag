e","source":["import os\n","import requests\n","from bs4 import BeautifulSoup\n","import time\n","import random\n","from urllib.parse import urljoin, urlparse\n","from google.colab import drive\n","from tqdm.notebook import tqdm\n","\n","drive.mount('/content/drive')\n","\n","def create_directory_structure(company_name):\n","    \"\"\"Create the directory structure in the current folder of the mounted Google Drive.\"\"\"\n","    base_dir = \"/content/drive/My Drive/MSDS490_Project/PR\"\n","    os.makedirs(base_dir, exist_ok=True)\n","\n","    company_dir = os.path.join(base_dir, company_name)\n","    os.makedirs(company_dir, exist_ok=True)\n","\n","    return company_dir\n","\n","def extract_company_name(url):\n","    \"\"\"Extract company name from URL.\"\"\"\n","    parsed_url = urlparse(url)\n","    domain = parsed_url.netloc.replace('www.', '')\n","    return domain.split('.')[0]\n","\n","def scrape_press_releases(url, max_articles=15):\n","    \"\"\"Scrape press releases from a given URL and save them to Google Drive.\"\"\"\n","    company_name = extract_company_name(url)\n","    print(f\"Scraping press releases for {company_name}...\")\n","\n","    company_dir = create_directory_structure(company_name)\n","\n","    try:\n","        response = requests.get(url)\n","        response.raise_for_status()\n","    except requests.exceptions.RequestException as e:\n","        print(f\"Error fetching the URL: {e}\")\n","        return\n","\n","    soup = BeautifulSoup(response.text, 'html.parser')\n","    article_links = []\n","\n","    for link in soup.find_all('a', href=True):\n","        full_url = urljoin(url, link['href'])\n","        if ('/newsroom/' in full_url and full_url not in article_links) | ('news' in full_url and full_url not in article_links) | ('press' in full_url and full_url not in article_links) | ('blog' in full_url and full_url not in article_links):\n","            article_links.append(full_url)\n","\n","    print(f\"Found {len(article_links)} potential articles.\")\n","    saved_articles = 0\n","    index = 0\n","\n","    with tqdm(desc='Scraping Articles', unit='article') as pbar:\n","        while saved_articles < max_articles and index < len(article_links):\n","            article_url = article_links[index]\n","            index += 1\n","\n","            try:\n","                # Add a randomized sleep time between 1 to 3 seconds to avoid getting blocked\n","                time.sleep(random.uniform(1, 3))\n","\n","                article_response = requests.get(article_url)\n","                article_response.raise_for_status()\n","                article_soup = BeautifulSoup(article_response.text, 'html.parser')\n","\n","                title_element = article_soup.find(['h1', 'h2', 'title'])\n","                title = title_element.text.strip() if title_element else f\"Article_{saved_articles+1}\"\n","                filename = \"\".join(c if c.isalnum() or c in [' ', '-', '_'] else '_' for c in title)[:50] + \".txt\"\n","\n","                paragraphs = article_soup.find_all('p')\n","                content = \"\\n\\n\".join([p.text.strip() for p in paragraphs])\n","\n","                if len(content.encode('utf-8')) < 1024:  # Skip files smaller than 1 KB\n","                    continue\n","\n","                file_path = os.path.join(company_dir, filename)\n","                with open(file_path, 'w', encoding='utf-8') as f:\n","                    f.write(f\"Title: {title}\\nURL: {article_url}\\n\\n{content}\")\n","\n","                saved_articles += 1\n","                pbar.update(1)\n","\n","            except Exception as e:\n","                print(f\"Error processing article {article_url}: {e}\")\n","\n","    print(f\"Scraping completed. {saved_articles} valid articles saved in {company_dir}\")\n","\n","# Usage example\n","url = input(\"Enter the press release page URL: \")\n","scrape_press_releases(url)\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":156,