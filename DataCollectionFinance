ent/drive')\n","        self.base_path = '/content/drive/MyDrive/MSDS490_Project/SEC_Data'\n","        os.makedirs(self.base_path, exist_ok=True)\n","\n","    def get_cik_mapping(self):\n","        \"\"\"Get CIK numbers for company tickers\"\"\"\n","        response = requests.get(\n","            'https://www.sec.gov/files/company_tickers.json',\n","            headers=self.headers\n","        )\n","        ticker_to_cik = {}\n","        data = response.json()\n","\n","        for entry in data.values():\n","            if entry['ticker'] in self.company_tickers:\n","                # Pad CIK with leading zeros to 10 digits\n","                ticker_to_cik[entry['ticker']] = str(entry['cik_str']).zfill(10)\n","\n","        return ticker_to_cik\n","\n","    def get_company_facts(self, cik):\n","        \"\"\"\n","        Get company facts from SEC API\n","\n","        Args:\n","            cik (str): Company CIK number\n","        \"\"\"\n","        url = f'https://data.sec.gov/api/xbrl/companyfacts/CIK{cik}.json'\n","        response = requests.get(url, headers=self.headers)\n","\n","        if response.status_code == 200:\n","            return response.json()\n","        else:\n","            print(f\"Error fetching data for CIK {cik}: {response.status_code}\")\n","            return None\n","\n","    def extract_financial_metrics(self, facts_data):\n","        \"\"\"\n","        Extract key financial metrics from company facts\n","\n","        Args:\n","            facts_data (dict): Company facts JSON data\n","        \"\"\"\n","        metrics = {\n","            'us-gaap': [\n","                'Assets',\n","                'Liabilities',\n","                'StockholdersEquity',\n","                'Revenues',\n","                'NetIncomeLoss',\n","                'OperatingIncomeLoss',\n","                'CashAndCashEquivalentsAtCarryingValue'\n","            ]\n","        }\n","\n","        extracted_data = {}\n","\n","        if facts_data and 'facts' in facts_data:\n","            for taxonomy in metrics:\n","                if taxonomy in facts_data['facts']:\n","                    for metric in metrics[taxonomy]:\n","                        if metric in facts_data['facts'][taxonomy]:\n","                            extracted_data[metric] = facts_data['facts'][taxonomy][metric]['units'].get('USD', [])\n","\n","        return extracted_data\n","\n","    def save_data(self, ticker, data):\n","        \"\"\"\n","        Save extracted data to Google Drive\n","\n","        Args:\n","            ticker (str): Company ticker symbol\n","            data (dict): Extracted financial data\n","        \"\"\"\n","        # Create company-specific directory\n","        company_path = os.path.join(self.base_path, ticker)\n","        os.makedirs(company_path, exist_ok=True)\n","\n","        # Save raw JSON\n","        raw_path = os.path.join(company_path, 'raw_data.json')\n","        with open(raw_path, 'w') as f:\n","            json.dump(data, f, indent=4)\n","\n","        # Save processed data as CSV\n","        if 'facts' in data:\n","            processed_data = self.extract_financial_metrics(data)\n","            processed_path = os.path.join(company_path, 'financial_metrics.json')\n","            with open(processed_path, 'w') as f:\n","                json.dump(processed_data, f, indent=4)\n","\n","    def extract_all_companies(self):\n","        \"\"\"Extract and save data for all companies\"\"\"\n","        cik_mapping = self.get_cik_mapping()\n","\n","        for ticker, cik in cik_mapping.items():\n","            print(f\"Processing {ticker}...\")\n","            facts_data = self.get_company_facts(cik)\n","\n","            if facts_data:\n","                self.save_data(ticker, facts_data)\n","                # Respect SEC's rate limits\n","                sleep(0.1)\n","\n","            print(f\"Completed processing {ticker}\")\n","\n"],"metadata":{"id":"diJx0x3BUDWP"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Example usage\n","companies = ['AAPL', 'MSFT', 'GOOGL', 'AMZN', 'META', 'NVDA', 'TSLA', 'JPM', 'RTX', 'WMT', 'V']\n","extractor = SECDataExtractor(companies, 'cadenwebb2024@u.northwestern.edu')\n","extractor.extract_all_companies()"],"metadata":{"id":"kWzwYB61VEAw","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1740089342711,"user_tz":300,"elapsed":14577,"user":{"displayName":"Caden Webb","userId":"00439241620104196769"}},"outputId":"5603120a-1cbe-4be4-d8be-0e725649a5d9"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Drive already mounted at /content/drive; to attempt to forcibly remount, call drive.mount(\"/content/drive\", force_remount=True).\n","Processing AAPL...\n","Completed processing AAPL\n","Processing NVDA...\n","Completed processing NVDA\n","Processing MSFT...\n","Completed processing MSFT\n","Processing AMZN...\n","Completed processing AMZN\n","Processing GOOGL...\n","Completed processing GOOGL\n","Processing META...\n","Completed processing META\n","Processing TSLA...\n","Completed processing TSLA\n","Processing WMT...\n","Completed processing WMT\n","Processing JPM...\n","Completed processing JPM\n","Processing V...\n","Completed processing V\n","Processing RTX...\n","Completed processing RTX\n"]}]},{"cell_type":"markdown","source":["## Briefly explore data"],"metadata":{"id":"U5_k-elTcYyh"}},{"cell_type":"code","source":["class SECDataAnalyzer:\n","    def __init__(self, base_path='/content/drive/MyDrive/MSDS490_Project/SEC_Data'):\n","        \"\"\"\n","        Initialize the SEC data analyzer\n","\n","        Args:\n","            base_path (str): Path to the SEC data directory\n","        \"\"\"\n","        self.base_path = base_path\n","\n","    def load_company_data(self, ticker):\n","        \"\"\"\n","        Load financial metrics for a specific company\n","\n","        Args:\n","            ticker (str): Company ticker symbol\n","        \"\"\"\n","        metrics_path = os.path.join(self.base_path, ticker, 'financial_metrics.json')\n","        with open(metrics_path, 'r') as f:\n","            return json.load(f)\n","\n","    def create_time_series_df(self, data, metric):\n","        \"\"\"\n","        Create a time series DataFrame for a specific metric\n","\n","        Args:\n","            data (dict): Company financial data\n","            metric (str): Metric name to analyze\n","        \"\"\"\n","\n","        # print(f\"Processing metric: {metric}\")  # Print the current metric\n","        # print(f\"Data for metric: {data.get(metric)}\")  # Print the data for the metric\n","\n","        # if metric not in data:\n","        #     return pd.DataFrame()\n","\n","        if metric not in data or not isinstance(data[metric], list):\n","            return pd.DataFrame()  # Ensure it's a list before processing\n","\n","        records = []\n","        for entry in data[metric]:  # Iterate directly over the list\n","            records.append({\n","                'date': entry.get('end'),\n","                'value': entry.get('val'),\n","                'form': entry.get('form'),\n","                'period': entry.get('fp')\n","            })\n","\n","        # records = []\n","        # for entry in data[metric]['units'].get('USD', []):\n","        #     records.append({\n","        #         'date': entry['end'],\n","        #         'value': entry['val'],\n","        #         'form': entry['form'],\n","        #         'period': entry['fp']\n","        #     })\n","\n","        df = pd.DataFrame(records)\n","        df['date'] = pd.to_datetime(df['date'])\n","        return df.sort_values('date')\n","\n","    def analyze_company_metrics(self, ticker):\n","        \"\"\"\n","        Analyze key metrics for a company\n","\n","        Args:\n","            ticker (str): Company ticker symbol\n","        \"\"\"\n","        data = self.load_company_data(ticker)\n","\n","        # Create DataFrames for each metric\n","        metrics = {}\n","        for metric in ['Assets', 'Liabilities', 'Revenues', 'NetIncomeLoss']:\n","            df = self.create_time_series_df(data, metric)\n","            if not df.empty:\n","                metrics[metric] = df\n","\n","        return metrics\n","\n","    def calculate_financial_ratios(self, ticker):\n","        \"\"\"\n","        Calculate key financial ratios\n","\n","        Args:\n","            ticker (str): Company ticker symbol\n","        \"\"\"\n","        data = self.load_company_data(ticker)\n","\n","        # Get annual 10-K data only\n","        assets_df = self.create_time_series_df(data, 'Assets')\n","        revenue_df = self.create_time_series_df(data, 'Revenues')\n","        income_df = self.create_time_series_df(data, 'NetIncomeLoss')\n","\n","        # Filter for annual reports\n","        assets_annual = assets_df[assets_df['form'] == '10-K']\n","        revenue_annual = revenue_df[revenue_df['form'] == '10-K']\n","        income_annual = income_df[income_df['form'] == '10-K']\n","\n","        # Calculate ratios\n","        ratios = pd.DataFrame()\n","        if not assets_annual.empty and not revenue_annual.empty:\n","            ratios['year'] = assets_annual['date'].dt.year\n","            ratios['asset_turnover'] = revenue_annual['value'] / assets_annual['value']\n","        if not income_annual.empty and not revenue_annual.empty:\n","            ratios['profit_margin'] = income_annual['value'] / revenue_annual['value']\n","\n","        return ratios\n","\n","    def plot_metric_trends(self, ticker, metrics_data):\n","        \"\"\"\n","        Plot trends for multiple metrics\n","\n","        Args:\n","            ticker (str): Company ticker symbol\n","            metrics_data (dict): Dictionary of metric DataFrames\n","        \"\"\"\n","        plt.figure(figsize=(15, 10))\n","\n","        for metric, df in metrics_data.items():\n","            if not df.empty:\n","                plt.plot(df['date'], df['value'], label=metric, marker='o')\n","\n","        plt.title(f'{ticker} Financial Metrics Over Time')\n","        plt.xlabel('Date')\n","        plt.ylabel('USD')\n","        plt.legend()\n","        plt.xticks(rotation=45)\n","        plt.grid(True)\n","        plt.tight_layout()\n","\n","    def compare_companies(self, companies, metric):\n","        \"\"\"\n","        Compare a specific metric across multiple companies\n","\n","        Args:\n","            companies (list): List of company tickers\n","            metric (str): Metric to compare\n","        \"\"\"\n","        plt.figure(figsize=(15, 8))\n","\n","        for ticker in companies:\n","            data = self.load_company_data(ticker)\n","            df = self.create_time_series_df(data, metric)\n","\n","            if not df.empty:\n","                # Filter for annual data\n","                annual_data = df[df['form'] == '10-K']\n","                plt.plot(annual_data['date'], annual_data['value'],\n","                        label=ticker, marker='o')\n","\n","        plt.title(f'{metric} Comparison Across Companies')\n","        plt.xlabel('Date')\n","        plt.ylabel('USD')\n","        plt.legend()\n","        plt.xticks(rotation=45)\n","        plt.grid(True)\n","        plt.tight_layout()\n","\n"],"metadata":{"id":"qjlYl4oscbrS"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Example usage\n","analyzer = SECDataAnalyzer()\n","\n","# Analyze a single company\n","ticker = 'RTX'\n","metrics_data = analyzer.analyze_company_metrics(ticker)\n","analyzer.plot_metric_trends(ticker, metrics_data)\n","\n","# Calculate and display financial ratios\n","ratios = analyzer.calculate_financial_ratios(ticker)\n","print(f\"\\nFinancial Ratios for {ticker}:\")\n","print(ratios)\n","\n","# Compare companies\n","companies = ['AAPL', 'MSFT', 'GOOGL']\n","analyzer.compare_companies(companies, 'Revenues')\n","\n","# Display summary statistics\n","for metric, df i